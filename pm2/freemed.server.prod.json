{
  "apps" : [{
    "name": "freemed.server.prod",
    "script": "Main.js",
    "ignore_watch" : ["node_modules", "logs"],
    "env": {
      "NODE_ENV": "production"
    },
    "merge_logs": true,
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "error_file": "./logs/freemed.server.prod.stderr.log",
    "out_file": "./logs/freemed.server.prod.stdout.log",
    "pid_file": "./logs/freemed.server.prod.pid"
  }]
}
